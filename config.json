{
  "deployment": {
    "replit_app_name": "silverfood-analyzer",
    "replit_username": "your-username",
    "use_deployment_url": true,
    "manual_override_url": "https://727cfeaf-e405-4336-bdae-5b555824a2d0-00-ceo3089p1ojz.janeway.replit.dev"
  },
  "health_tips": {
    "interval_seconds": 4,
    "show_tips": true,
    "fade_duration_ms": 500
  },
  "ui": {
    "default_url": "https://www.ah.nl/allerhande/recept/R-R1201256/orzosalade-met-asperges-nectarines-en-burrata",
    "max_url_length": 500,
    "enable_url_validation": true,
    "show_loading_messages": true,
    "scroll_to_results": true
  },
  "analysis": {
    "min_ingredients_required": 3,
    "max_ingredients_display": 20,
    "selenium_retry_attempts": 2,
    "requests_retry_attempts": 2,
    "page_load_timeout_seconds": 30,
    "selenium_wait_seconds": 15
  },
  "api": {
    "rate_limit_requests": 8,
    "rate_limit_window_seconds": 60,
    "enable_debug_logging": true,
    "max_concurrent_requests": 5
  },
  "scraping": {
    "random_delay_min_seconds": 1,
    "random_delay_max_seconds": 3,
    "user_agent_rotation": true,
    "enable_anti_detection": true,
    "selenium_single_process": true
  },
  "health_scoring": {
    "use_openfoodfacts_api": true,
    "api_timeout_seconds": 10,
    "cache_nutrition_data": true,
    "default_unknown_score": 5
  }
